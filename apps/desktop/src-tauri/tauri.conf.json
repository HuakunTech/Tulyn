{
	"productName": "Kunkun",
	"version": "0.1.3",
	"identifier": "sh.kunkun.desktop",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://localhost:3000",
		"beforeBuildCommand": "pnpm generate",
		"frontendDist": "../dist"
	},
	"app": {
		"macOSPrivateApi": true,
		"windows": [
			{
				"title": "",
				"url": "init-page",
				"label": "main",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false,
				"titleBarStyle": "Transparent",
				"transparent": false,
				"hiddenTitle": true,
				"visible": false
			}
		],
		"security": {
			"csp": null,
			"assetProtocol": {
				"enable": true,
				"scope": ["**"]
			}
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {
		"updater": {
			"createUpdaterArtifacts": true,
			"endpoints": ["https://updater.kunkun.sh"],
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDc1NENCRjZFM0JBOEQ0ODMKUldTRDFLZzdicjlNZFhHS0ZKYk13WkdZUTFUM01LNjkvVW5Bb2x1SnB1R0crbFRuMnlRSlJ0STgK"
		}
	}
}
