{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "ext-capability",
  "description": "Capability for the extension window",
  "windows": [
    "ext*"
  ],
  "remote": {
    "urls": [
      "http://localhost:1566/*",
      "http://localhost:*"
    ]
  },
  "local": true,
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-create",
    "window:allow-start-dragging",
    "window:allow-destroy",
    "webview:default",
    "http:allow-fetch",
    "webview:allow-create-webview",
    "webview:allow-create-webview-window",
    "app:default",
    "resources:default",
    "image:default",
    "menu:default",
    "tray:default",
    "shell:default",
    "log:default",
    "global-shortcut:default",
    "fs:default",
    "os:default",
    "shell:allow-open",
    "log:allow-log",
    "fs:allow-exists",
    "fs:allow-stat",
    "fs:read-all",
    "fs:allow-mkdir",
    "fs:allow-app-write",
    "fs:allow-app-write-recursive",
    "fs:allow-appcache-write",
    "fs:allow-appcache-write-recursive",
    "fs:allow-appconfig-write",
    "os:allow-platform",
    "notification:default",
    "notification:allow-is-permission-granted",
    "notification:allow-notify",
    "notification:allow-request-permission",
    "dialog:default",
    "dialog:allow-confirm",
    "dialog:allow-ask",
    "dialog:allow-open",
    "store:default",
    "store:allow-clear",
    "store:allow-delete",
    "store:allow-entries",
    "store:allow-get",
    "store:allow-has",
    "store:allow-keys",
    "store:allow-length",
    "store:allow-load",
    "store:allow-reset",
    "store:allow-save",
    "store:allow-set",
    "store:allow-values",
    "store:default",
    "log:default"
  ]
}