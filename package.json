{
  "name": "jarvis",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "develop": "turbo run develop",
    "commit": "pnpm gitmoji -c",
    "lint": "turbo run lint",
    "format:apps": "prettier --write \"./apps/**/*.{ts,tsx,md,vue,json,yaml,yml}\"",
    "format:packages": "prettier --write \"./packages/**/*.{ts,tsx,md,vue,json,yaml,yml}\"",
    "format": "npm run format:apps && npm run format:packages",
    "prepare": "husky"
  },
  "devDependencies": {
    "@dep-tree/cli": "^0.23.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "latest",
    "@types/node": "^20.14.11",
    "esbuild": "^0.23.0",
    "eslint": "^8.48.0",
    "gitmoji-cli": "^9.4.0",
    "husky": "^9.0.11",
    "madge": "^7.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "turbo": "^2.0.7",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.5.0",
  "dependencies": {
    "@huakunshen/comlink": "^4.4.1",
    "tauri-plugin-clipboard-api": "^2.1.2",
    "tauri-plugin-shellx-api": "^2.0.8",
    "valibot": "^0.35.0"
  }
}