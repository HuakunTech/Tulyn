{
	"name": "kunkun",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "turbo run build",
		"test": "turbo run test",
		"dev": "turbo run dev",
		"develop": "turbo run develop",
		"lint": "turbo run lint",
		"format:apps": "bun --bun prettier --write \"./apps/**/*.{ts,tsx,md,vue,json,yaml,yml,svelte}\"",
		"format:packages": "bun --bun prettier --write \"./packages/**/*.{ts,tsx,md,vue,json,yaml,yml,svelte}\"",
		"format": "npm run format:apps && npm run format:packages",
		"prepare": "husky",
		"commitlint": "commitlint --edit",
		"commit": "commit"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.7",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@commitlint/prompt-cli": "^19.3.1",
		"@dep-tree/cli": "^0.23.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.3.0",
		"@types/bun": "latest",
		"@types/node": "^20.14.11",
		"esbuild": "^0.23.0",
		"eslint": "^8.48.0",
		"husky": "^9.0.11",
		"madge": "^7.0.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"turbo": "^2.0.14",
		"typescript": "^5.4.5"
	},
	"packageManager": "pnpm@9.8.0",
	"dependencies": {
		"@huakunshen/comlink": "^4.4.1",
		"tauri-plugin-clipboard-api": "^2.1.6",
		"tauri-plugin-shellx-api": "^2.0.8",
		"valibot": "^0.36.0"
	}
}
