---
import {ShellPermissionMap} from '@kksh/api/permissions'
---

<ul>
	{
		Object.entries(ShellPermissionMap).map(([methodName, permissions]) => (
			<li>
				<code class="">{methodName}</code>:
				<span class="gap-1">
					[
					{permissions.map((p) => (
						<code>{p}</code><span> </span>
					))}
					]
				</span>
			</li>
		))
	}
</ul>
