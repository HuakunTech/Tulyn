{
	"$schema": "./node_modules/@kksh/api/dist/schema.json",
	"name": "template-ext-worker",
	"version": "0.0.2",
	"type": "module",
	"kunkun": {
		"name": "Extension Template Worker",
		"shortDescription": "A Worker Extension Template",
		"longDescription": "A Worker Extension Template",
		"identifier": "template-ext-worker",
		"permissions": [
			"fetch:all",
			"clipboard:read-all",
			{
				"permission": "shell:execute",
				"allow": [
					{
						"cmd": {
							"program": "bash",
							"args": [
								"-c",
								".+"
							]
						}
					}
				]
			}
		],
		"demoImages": [],
		"icon": {
			"type": "iconify",
			"value": "material-symbols:extension"
		},
		"customUiCmds": [],
		"templateUiCmds": [
			{
				"name": "Worker Template",
				"main": "dist/index.js",
				"cmds": []
			}
		]
	},
	"scripts": {
		"dev": "rollup -c --watch",
		"build": "rimraf dist && rollup -c"
	},
	"dependencies": {
		"@kksh/api": "workspace:*"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/bun": "latest",
		"rimraf": "^6.0.1",
		"rollup": "^4.18.1",
		"rollup-plugin-visualizer": "^5.12.0"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"files": [
		"./dist",
		".gitignore"
	]
}
