{
	"$schema": "../../packages/schema/manifest-json-schema.json",
	"name": "template-ext-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"kunkun": {
		"name": "template-ext-svelte",
		"shortDescription": "A Custom UI template for svelte",
		"longDescription": "A Custom UI template for svelte",
		"identifier": "template-ext-svelte",
		"icon": {
			"type": "iconify",
			"value": "logos:svelte-icon"
		},
		"demoImages": [],
		"permissions": [
			"clipboard:read-text",
			"notification:all"
		],
		"customUiCmds": [
			{
				"main": "dist",
				"devMain": "http://localhost:5173",
				"name": "Svelte Template",
				"cmds": [],
				"window": {
					"title": "Svelte Template"
				}
			}
		],
		"templateUiCmds": []
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json"
	},
	"dependencies": {
		"@kksh/api": "workspace:*",
		"@kksh/svelte": "0.1.4",
		"clsx": "^2.1.1",
		"lucide-svelte": "^0.416.0",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tsconfig/svelte": "^5.0.4",
		"autoprefixer": "^10.4.19",
		"postcss": "^8.4.38",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.3",
		"typescript": "^5.2.2",
		"vite": "^5.3.4"
	},
	"files": [
		"dist"
	]
}
