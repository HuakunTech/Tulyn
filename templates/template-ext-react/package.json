{
  "$schema": "../../packages/schema/manifest-json-schema.json",
  "name": "template-ext-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "kunkun": {
    "name": "template-ext-react",
    "shortDescription": "A Custom UI template for react",
    "longDescription": "A Custom UI template for react",
    "identifier": "sh.kunkun-ext-custom-react",
    "icon": {
      "type": "iconify",
      "value": "logos:react"
    },
    "demoImages": [],
    "permissions": [],
    "customUiCmds": [
      {
        "main": "dist",
        "devMain": "http://localhost:5173",
        "name": "React Extension Template",
        "cmds": [],
        "window": {
          "title": "React Extension Template in new Window"
        }
      }
    ],
    "templateUiCmds": []
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@kksh/api": "workspace:*",
    "@kksh/react": "workspace:*",
    "@radix-ui/react-icons": "^1.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
